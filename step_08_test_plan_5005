SELECT              
  cpi,
  acctnum,
  npi1,
  lname,
  fname,
  addr2,
  city,
  state,
  charges,
  balance
FROM temp_batch_insurance0_unstaged_t
--WHERE cpi = 'E1503798226'
--AND acctnum = '58000019305';
LIMIT 20;

