++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

SELECT 
  id_rec,
  dataload->>'cpi' as cpi,
  dataload->>'acctnum' as acctnum,
  dataload->>'charges' as charges,
  dataload->>'balance' as balance
FROM unstaged_t
WHERE dataload->>'hid' IS NOT NULL;


SELECT 
COUNT(*)
FROM unstaged_t
WHERE dataload->>'hid' IS NOT NULL;

