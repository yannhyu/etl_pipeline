        | sed '$d' | sed -e 's/[^-a-zA-Z0-9,#"\. ]//g' \
        | ${MNTBIN}/convert_csv_to_text.pl | sed -e '/^[^3]/ d' \
        | ${BIN}/insurance0/convert_insurance0_v1.pl -b ${BATCH_RECV_DATE} \
        | ${PGSQL} ${SQL_RUNT_DIR}/load_table_batch_temp_insurance0.sql
