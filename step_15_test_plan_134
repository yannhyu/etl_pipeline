SELECT 
  our_run_date,
  dob,
  g_dob,
  service_date_begin,
  service_date_end,
  medicaid_max_date,
  medicare_max_date,
  third_party_sent_date,
  emdeon_sent_date,
  client_date1,
  dob_corrected,
  drop_date
FROM temp_batch_insurance0_unstaged_t
LIMIT 20;

