SELECT 
  id_rec,
  seqnum,
  dataload->>'hid' as hid,
  dataload->>'cpi' as cpi,
  dataload->>'acctnum' as acctnum,
  dataload->>'charges' as charges,
  dataload->>'balance' as balance
FROM unstaged_t
--WHERE seqnum = 559825;
--WHERE seqnum >= 559810;
LIMIT 10;

